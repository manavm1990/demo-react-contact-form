    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "autoprefixer": "^10.4.13",
    "dependency-cruiser": "^12.5.0",
